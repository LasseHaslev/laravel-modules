{
    "name": "lassehaslev/laravel-modules",
    "type": "laravel-plugin",
    "authors": [
        {
            "name": "Lasse S. Haslev",
            "email": "lasse@haslev.no"
        }
    ],
    "require": {
        "wikimedia/composer-merge-plugin": "1.x"
    },
    "require-dev": {
        "orchestra/testbench": "3.3.x",
        "phpunit/phpunit": "^5.6"
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "tests/Modules/*/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false
        }
    },
    "autoload": {
        "psr-4": {
            "LasseHaslev\\LaravelModules\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
